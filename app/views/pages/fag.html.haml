#fag-page-container
  = render 'pages/content'

:javascript
  $('.question').click(function(){
    if ($(this).parent().hasClass('active')){
      $(this).parent().removeClass('active');
    }
    else{
      $(this).parent().addClass('active');
    }
  });
  $('#expand-block').click(function(){
    $('div.block').addClass('active');
  });
